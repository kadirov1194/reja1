<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Portfolio</title>
    <link rel="stylesheet" href="/main.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
      crossorigin="anonymous"
    ></script>
  </head>

  <body>
    <div class="frame">
      <div class="main_frame">
        <div class="left_frame">
          <div class="left_1">
            <div class="left_1_1"></div>
            <img src="images/profile.png" width="156px" height="135px" />

            <div class="left_1_2"><%=user.name%></div>
            <div class="left_1_3"><%=user.profession%></div>
            <div class="left_icons">
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon\Icons-dribbble.svg" class="icon-1" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon\Icons-facebook.svg" class="icon-2" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon\Icons-instagram.svg" class="icon-3" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon\Icons-linkedin.svg" class="icon-4" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon\Icons-twitter.svg" class="icon-5" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon\Icons-Youtube.svg" class="icon-6" />
                </div>
              </a>
            </div>
          </div>
          <div class="marginer"></div>

          <!--  Left_2 box boyicha amallar -->
          <div class="left_2">
            <div class="left_2-1">
              <div class="left_2_1">Age:</div>
              <div class="left_2_2">Residence:</div>
              <div class="left_2_3">Freelance:</div>
              <div class="left_2_4">Address:</div>
            </div>
            <!--  -->
            <div class="left_2-2">
              <div class="left_2_6"><%=user.age%></div>
              <div class="left_2_7"><%=user.residence%></div>
              <div class="left_2_8"><%=user.freelance%></div>
              <div class="left_2_9"><%=user.address%></div>
            </div>
          </div>

          <div class="marginer"></div>

          <!-- left_3 boyicha amallar  Languages-->

          <div class="left_3">
            <div class="languages">Languages</div>
            <div class="bangla">
              <div class="bangla-1"><%=user.languages[0]%></div>
              <div class="bangla-2">100%</div>
            </div>

            <div class="lan-border">
              <div class="percent-1"></div>
            </div>

            <div class="bangla">
              <div class="bangla-1"><%=user.languages[1]%></div>
              <div class="bangla-2">80%</div>
            </div>

            <div class="lan-border"><div class="percent-2"></div></div>
            <div class="bangla">
              <div class="bangla-1"><%=user.languages[2]%></div>
              <div class="bangla-2">60%</div>
            </div>
            <div class="lan-border"><div class="percent-3"></div></div>
          </div>
          <div class="marginer"></div>
          <!-- ============================= -->
          <!--  -->
          <!-- SKILLS -->
          <div class="left_4">
            <div class="languages">Skills</div>
            <div class="bangla">
              <div class="bangla-1"><%=user.skills[0]%></div>
              <div class="bangla-2">90%</div>
            </div>
            <div class="lan-border">
              <div class="percent-html"></div>
            </div>
            <div class="bangla">
              <div class="bangla-1"><%=user.skills[1]%></div>
              <div class="bangla-2">85%</div>
            </div>
            <div class="lan-border"><div class="percent-2"></div></div>
            <div class="bangla">
              <div class="bangla-1"><%=user.skills[2]%></div>
              <div class="bangla-2">80%</div>
            </div>
            <div class="lan-border"><div class="percent-2"></div></div>
            <div class="bangla">
              <div class="bangla-1"><%=user.skills[3]%></div>
              <div class="bangla-2">75%</div>
            </div>
            <div class="lan-border"><div class="percent-nodejs"></div></div>
            <div class="bangla">
              <div class="bangla-1"><%=user.skills[4]%></div>
              <div class="bangla-2">85%</div>
            </div>
            <div class="lan-border"><div class="percent-2"></div></div>
          </div>
          <!-- END SKILLS -->
          <div class="marginer"></div>
          <!--  -->
          <!-- EXTRA SKILLS -->
          <div class="left_5">
            <div class="ex-skills">Extra Skills</div>
            <div class="ex-skill-text">
              <div class="border">
                <div class="border-1"></div>
                <div class="border-2"></div>
              </div>
              <div class="text"><%=user.extra_skills[0]%></div>
            </div>
            <div class="ex-skill-text">
              <div class="border">
                <div class="border-1"></div>
                <div class="border-2"></div>
              </div>
              <div class="text"><%=user.extra_skills[1]%></div>
            </div>
            <div class="ex-skill-text">
              <div class="border">
                <div class="border-1"></div>
                <div class="border-2"></div>
              </div>
              <div class="text"><%=user.extra_skills[2]%></div>
            </div>
            <div class="ex-skill-text">
              <div class="border">
                <div class="border-1"></div>
                <div class="border-2"></div>
              </div>
              <div class="text"><%=user.extra_skills[3]%></div>
            </div>
          </div>
          <!-- END EXTRA SKILLS -->
          <div class="marginer"></div>
        </div>
        <!-- ********************************************* -->
        <!-- MIDD FRAME -->
        <div class="midd_frame">
          <div class="midd_top">
            <div class="midd_top_div">
              <div class="midd_top_frame">
                <!-- ROUND -->
                <div class="round_yellow"></div>
                <div class="round_green"></div>
                <div class="round_yellow_1"></div>
                <div class="round_green_1"></div>
                <div class="rectangle_yellow"></div>
                <div class="rectangle_blue"></div>
                <div class="triangle_red"></div>
                <!-- END ROUND -->
                <div class="midd_top_main_text">
                  <%=user.profession%> in <%=user.work_place%>
                </div>
                <div class="midd_top_text"><%=user.about_me%></div>
                <a class="link_href" href="">
                  <div class="midd_top_btn">
                    <div class="btn_text">HIRE ME</div>
                    <div class="btn_icon">
                      <img src="btn_icon/Vector.svg" alt="" />
                    </div>
                  </div>
                </a>
              </div>
              <div class="midd_top_img">
                <img src="images/asosiy.png" alt="" />
              </div>
            </div>
          </div>
          <div class="midd_bot">
            <div class="midd_bot_main_txt">My Services</div>
            <div class="midd_bot_text"><%=user.service_desc%></div>
            <div class="midd_boxs">
              <div class="boxs">
                <div class="box">
                  <div class="box_img">
                    <img src="box_images/Vector.svg" alt="" />
                    <img
                      class="coding_icon_left"
                      src="box_images/Vector_1_2.svg"
                      alt=""
                    />
                    <img
                      class="coding_icon_midd"
                      src="box_images/Vector_1.svg"
                      alt=""
                    />
                    <img
                      class="coding_icon_right"
                      src="box_images/Vector_1_1.svg"
                      alt=""
                    />
                  </div>
                  <div class="box_main_txt">Web Development</div>
                  <div class="box_text">BLog, E-commerce</div>
                </div>
              </div>
              <div class="boxs">
                <div class="box">
                  <div class="box_img">
                    <img src="box_images/Vector-4.svg" alt="" />
                  </div>
                  <div class="box_main_txt">UI/UX Design</div>
                  <div class="box_text">Mobile App, Website Design</div>
                </div>
              </div>
              <div class="boxs">
                <div class="box">
                  <div class="box_img">
                    <img src="box_images/Vector-5.svg" alt="" />
                  </div>
                  <div class="box_main_txt">Sound Desigin</div>
                  <div class="box_text">Voice Over, Beat Making</div>
                </div>
              </div>
            </div>
            <div class="midd_boxs">
              <div class="boxs">
                <div class="box">
                  <div class="box_img">
                    <img src="box_images/game.svg" alt="" />
                  </div>
                  <div class="box_main_txt">Mobile App Design</div>
                  <div class="box_text">Character Design, Back End</div>
                </div>
              </div>
              <div class="boxs">
                <div class="box">
                  <div class="box_img">
                    <img src="box_images/Vector-2.svg" alt="" />
                  </div>
                  <div class="box_main_txt">Photography</div>
                  <div class="box_text">Portrait, Product PhotoGraphy</div>
                </div>
              </div>
              <div class="boxs">
                <div class="box">
                  <div class="end_box_main_text">Advertising</div>
                  <div class="end_box_text">
                    orem Ipsum is simply dummy text of the printing and
                    typesetting industry typesetting industry industry.
                  </div>
                  <a class="link_href" href="">
                    <div class="end_box_link">
                      <div>Order Now</div>
                      <div>
                        <img
                          class="end_box_icon"
                          src="box_images/end_box.svg"
                          alt=""
                        />
                      </div>
                    </div>
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- END MIDD FRAME -->

        <!-- **********************************************
        
        -->
        <!-- RIGHT FRAME -->
        <div class="right_frame">
          <a href="">
            <div class="vector">
              <img src="menu_icons/icons/Vector.svg" alt="" />
            </div>
          </a>

          <div class="menu">
            <a class="link_href" href="">
              <div class="menu_icon_border">
                <span class="tooltip">Home</span>
                <img src="menu_icons/icons/Vector-1.svg" alt="" />
              </div>
            </a>
            <a class="link_href" href="">
              <div class="menu_icon_border">
                <span class="tooltip">coding</span>
                <img src="menu_icons/Vector.svg" alt="" />
              </div>
            </a>
            <a class="link_href" href="">
              <div class="menu_icon_border">
                <span class="tooltip">users</span>
                <img src="menu_icons/Vector-1.svg" alt="" />
              </div>
            </a>
            <a class="link_href" href="">
              <div class="menu_icon_border">
                <span class="tooltip">store</span>
                <img src="menu_icons/icons/Vector-2.svg" alt="" />
              </div>
            </a>
            <a class="link_href" href="">
              <div class="menu_icon_border">
                <span class="tooltip">article</span>
                <img src="menu_icons/icons/Vector-3.svg" alt="" />
              </div>
            </a>
            <a class="link_href" href="">
              <div class="menu_icon_border">
                <span class="tooltip">chat</span>
                <img src="menu_icons/icons/Vector-4.svg" alt="" />
              </div>
            </a>
          </div>
        </div>
        <!-- END RIGHT FRAME -->
      </div>
    </div>
  </body>
</html>
